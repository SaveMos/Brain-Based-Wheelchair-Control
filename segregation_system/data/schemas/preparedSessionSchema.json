{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "uuid": {
        "type": "string"
      },
      "label": {
        "type": "string"
      },
      "psd_alpha_band": {
        "type": "number"
      },
      "psd_beta_band": {
        "type": "number"
      },
      "psd_theta_band": {
        "type": "number"
      },
      "psd_delta_band": {
        "type": "number"
      },
      "activity": {
        "type": "string",
        "enum": ["sport", "meditation", "work", "home", "relax"]
      },
      "genre": {
        "type": "string",
        "enum": ["pop", "classical", "rock", "disco", "workout"]
      }
    },
    "required": [
      "uuid",
      "label",
      "psd_alpha_band",
      "psd_beta_band",
      "psd_theta_band",
      "psd_delta_band",
      "activity",
      "genre"
    ]
  }