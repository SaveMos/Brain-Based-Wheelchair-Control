{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
   "development_phase": {
      "type": "string",
      "enum": ["True", "False"]
    },
    "sampling_frequency": {
      "type": "number"
    },
    "upper_bound": {
      "type": "number"
    },
    "lower_bound": {
      "type": "number"
    },
    "testing": {
      "type": "string",
      "enum": ["True", "False"]
    },
    "bandwidths": {
      "type": "object",
      "properties": {
        "psd_alpha_band": {
          "type": "array",
          "items": [
            {
              "type": "integer"
            },
            {
              "type": "integer"
            }
          ]
        },
        "psd_beta_band": {
          "type": "array",
          "items": [
            {
              "type": "integer"
            },
            {
              "type": "integer"
            }
          ]
        },
        "psd_theta_band": {
          "type": "array",
          "items": [
            {
              "type": "integer"
            },
            {
              "type": "integer"
            }
          ]
        },
        "psd_delta_band": {
          "type": "array",
          "items": [
            {
              "type": "integer"
            },
            {
              "type": "integer"
            }
          ]
        }
      },
      "required": [
        "psd_alpha_band",
        "psd_beta_band",
        "psd_theta_band",
        "psd_delta_band"
      ]
    }
  },
  "required": [
    "development_phase",
    "lower_bound",
    "testing",
    "sampling_frequency",
    "bandwidths",
    "upper_bound"
  ]
}